<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
      body{
    background-color:bisque;
}
table{
    background-color:darkcyan
}
#button{
    margin-top:20px;
    padding: 10px;
    font-size: 19px;
    background-color: darkcyan;
    color:wheat;
    border-radius: 50px;
}
#form1{
    max-width: 500px;
    justify-content: space-between;
    display: flex;
}
#form2{
    justify-content: space-between;
    display: flex;
    max-width: 1600px;
    margin-bottom: 20px;
}
    </style>
</head>
<body id="body">
  <div id="form">
    <div class="element">
      <h3>search : </h3>
      <form action="/dashboard/Delimitersearch/Delimitersearch_output" method="post">
        <input type="text" name="str" id="str" value="<%= qr %>" autofocus>
        <button type="submit">GO</button>
        <p id="err"></p>
      </form>
    </div>
  </div>
  <div class="table">
    <table border="1">
      <thead>
        <tr>
          <th>ID</th>
          <th>First Name (_)</th>
          <th>Last Name (^)</th>
          <th>Email ($)</th>
          <th>Email2</th>
          <th>Designation</th>
          <th>City (:)</th>
          <th>Country</th>
          <th>Phone number ({)</th>
          <th>Gender</th>
          <th>zipcode</th>
          <th>createdTime</th>
        </tr>
      </thead>
      <tbody>
        <% if (userData.length !=0) {%>
          <% userData.forEach(function(data, index) { %>
            <tr>
              <td>
                <%= data.stu_id%>
              </td>
              <td>
                <%= data.fname %>
              </td>
              <td>
                <%= data.lname %>
              </td>
              <td>
                <%= data.email %>
              </td>
              <td>
                <%= data.email2 %>
              </td>
              <td>
                <%= data.Designation %>
              </td>
              <td>
                <%= data.City%>
              </td>
              <td>
                <%= data.Country %>
              </td>
              <td>
                <%= data.Phonenum%>
              </td>
              <td>
                <%= data.Gender%>
              </td>
              <td>
                <%= data.zipcode%>
              </td>
              <td>
                <%= data.createdTime%>
              </td>
            </tr>
            <% }); %>
              <%} %>
      </tbody>
    </table>
  </div>
</body>
</html>